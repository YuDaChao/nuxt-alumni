{
  "name": "nuxt-alumni",
  "version": "1.0.0",
  "description": "My astounding Nuxt.js project",
  "author": "happyu",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
