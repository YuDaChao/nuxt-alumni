    "echarts": "^4.2.1",
    "element-ui": "~2.7.2",
    "koa": "^2.6.2",
    "node-sass": "^4.12.0",
    "nuxt": "^2.4.0",
    "sass-loader": "^7.1.0",
    "vue-echarts": "^4.0.3"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.9",
